{
  "name": "schematic-map",
  "version": "0.2.0",
  "description": "The IndoorJs Grid as a FabricJS library, facilitates working above coordinate planes",
  "type": "module",
  "main": "dist/schematic-map.cjs.js",
  "module": "dist/schematic-map.esm.js",
  "browser": "dist/schematic-map.umd.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "npm run build && (npm run watch & npm run serve)",
    "watch": "rollup -c -w",
    "serve": "browser-sync start --server --files 'dist/**/*.js,demo/**/*.html' --startPath demo/grid-demo.html",
    "dev:old": "browser-sync start --server --files 'src/**/*.js,demo/**/*.html' --startPath demo/grid-demo.html",
    "prepublishOnly": "yarn build",
    "publish:npm": "npm publish --access public",
    "publish:jsr": "npx jsr publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajoslin103/schematic-maps.git"
  },
  "keywords": [
    "fabric",
    "canvas",
    "grid",
    "schematic",
    "interactive"
  ],
  "author": "Allen Joslin <ajoslin103@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "fabric": "^6.7.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "browser-sync": "^3.0.4",
    "rollup": "^4.22.4"
  }
}
